class Program
{
    static void Main()
    {
        Console.Write("Zadej d√©lku strany : ");
        int delka = int.Parse(Console.ReadLine());

        Console.Write("Zadej znak: ");
        char znak = Console.ReadLine()[0];

        for (int i = 0; i < delka; i++){
            for (int j = 0; j < delka; j++){
                    Console.Write(znak);
                }
            Console.WriteLine();
        }

    }
}